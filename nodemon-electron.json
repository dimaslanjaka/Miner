{
  "restartable": "rs",
  "verbose": false,
  "exec": "electron -r ts-node/register src/electron/index.ts",
  "ignore": [".git", "node_modules"],
  "watch": ["src/electron/**"],
  "events": {
    "restart": "",
    "crash": ""
  },
  "execMap": {
    "js": "node --harmony",
    "ts": "node -r ts-node/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1500,
  "ext": "ts,js,html"
}
