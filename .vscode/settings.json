{
  "cSpell.words": [
    "Agustina",
    "Beaucachu",
    "Dimas",
    "dimaslanjaka",
    "webmanajemen",
    "Gitlab",
    "Goldbach",
    "Hexo",
    "INDO",
    "Lanjaka",
    "Maulinda",
    "Neverland",
    "Praha",
    "apgus",
    "browsersync",
    "delim",
    "epiphyllum",
    "filemanager",
    "genshin",
    "livereload",
    "maxdepth",
    "musique",
    "notranslate",
    "shortcode",
    "titlecase",
    "tlon",
    "youtube"
  ],
  "terminal.integrated.env.linux": {
    "PATH": "bin:${env:PATH}"
  },
  "files.autoSave": "onWindowChange",
  "files.autoSaveDelay": 30000,
  "typescript.tsc.autoDetect": "off",
  "gulptasks.pattern": "**/gulpfile.ts",
  "gulptasks.args": [
    "-r",
    "ts-node/register"
  ],
  "gulptasks.filters": [
    "!**/node_modules/**",
    "!**/bower_components/**",
    "!**/packages/**",
    "!**/themes/**"
  ],
  "npm.enableRunFromFolder": true,
  "javascript.suggestionActions.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "exclude/**": true,
    "**/.idea": true,
    "packages/hexo-admin": true,
    "packages/hexo-admin-ehc": true,
    "packages/hexo-toc": true,
    "packages/hexo-related*": true,
    "packages/hexo": true,
    "packages/common": true,
    "packages/ga-analytics": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/tmp/*/**": true,
    "**/build/*/**": true,
    "**/dist/*/**": true,
    "**/.hg/store/**": true,
    "docs/*/**": true,
  },
  "git.detectSubmodulesLimit": 10,
  "git.detectSubmodules": false
  //"editor.fontFamily": "Droid Sans Mono, Droid Sans Fallback"
}