{
  "name": "app",
  "version": "0.0.1",
  "description": "Node-Webkit Test",
  "chromium-args": "--remote-debugging-port=9222 --disable-raf-throttling --disable-features=nw2 -ignore-certificate-errors",
  "window": {
    "icon": "src/img/icon.png",
    "height": 600,
    "resizable": true,
    "width": 1000,
    "title": "Metronome_manifest_window_title"
  },
  "webview": {
    "partitions": [
      {
        "name": "web-session",
        "accessible_resources": [
          "<all_urls>"
        ]
      }
    ]
  },
  "author": "Dimas Lanjaka",
  "license": "mit",
  "private": true,
  "main": "src/index.html",
  "scripts": {
    "start": "node index.js",
    "start-nw": "nw --disable-raf-throttling"
  },
  "devDependencies": {
    "express": "^4.16.4",
    "nw": "^0.38.0-sdk"
  }
}